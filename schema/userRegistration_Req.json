{"schemaName": "userRegistrationSchema",
"schemaDescription": "To be used when handling User Registration Requests",
"schema": {
    "properties": {
        "emailAddress": {
            "format": "email",
            "type": "string"
        },
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        },
        "password": {
            "type": "string"
        }
    },
    "required": [
        "firstName",
        "lastName",
        "emailAddress",
        "password"
    ],
    "type": "object"
}
}