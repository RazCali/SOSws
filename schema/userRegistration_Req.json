{
    "aclMatrix": {
        "anonymous": ["AppUser"],
        "AppManager": ["AppResource"],
        "HeapAdmin": ["AppManager"]
    },
    "schema": {
        "additionalProperties": false,
        "properties": {
            "authToken": {
                "type": "string"
            },
            "emailAddress": {
                "format": "email",
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "password": {
                "type": "string"
            },
            "sessionId": {
                "type": "string"
            },
            "userId": {
                "type": "string"
            }
        },
        "required": [
            "name",
            "emailAddress",
            "password",
            "sessionId"
        ],
        "type": "object"
    },
    "schemaDescription": "To be used when handling User Registration Requests",
    "schemaName": "userRegistrationSchema"
}